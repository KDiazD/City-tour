<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="citytour.master"
             Title="Master">

    <StackLayout BackgroundColor="Black">
        <Button
            Text="Historia"
            x:Name="historia_neiva"
            Clicked="Historia_neiva_Clicked"
            BackgroundColor="Transparent"
            TextColor="White"
        />
        <Button
            Text="Comidas"
            x:Name="comidas_neiva"
            Clicked="Comidas_neiva_Clicked_1"
            BackgroundColor="Transparent"
            TextColor="White"
        />
        <Button
            Text="Lugares"
            x:Name="lugares_neiva"
            Clicked="Lugares_neiva_Clicked"
            BackgroundColor="Transparent"
            TextColor="White"
        />
        <Button
            Text="Fiestas"
            x:Name="fiestas_neiva"
            Clicked="Fiestas_neiva_Clicked"
            BackgroundColor="Transparent"
            TextColor="White"
        />
        

    </StackLayout>



</ContentPage>